{
  "name": "pauliustumosa/laravel-console-output",
  "description": "Tiny helper to write to Laravel log/console from any part of the application.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Your Name",
      "email": "you@example.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "symfony/console": "^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "^3.7",
    "phpstan/phpstan": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Tumosa\\LaravelConsoleOutput\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tumosa\\LaravelConsoleOutput\\LaravelConsoleOutputServiceProvider"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tumosa\\LaravelConsoleOutput\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
